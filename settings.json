		{
			"name": "vscode-expo-manifest",
			"url": "file:///Users/monaahy/Library/Application%20Support/Code/User/globalStorage/bycedric.vscode-expo/manifest-44.0.0.json",
			"fileMatch": ["app.json", "app.config.json"]
		}
	],
	"gitlens.defaultDateFormat": null,
	"editor.formatOnPaste": true,
	"react-native.packager.port": 19000,
	"react-native-tools.networkInspector.consoleLogsColorTheme": "Dark",
	"json.maxItemsComputed": 50000,
	"security.workspace.trust.untrustedFiles": "open",
	"code-runner.runInTerminal": true,
	"editor.renderControlCharacters": true,
	"diffEditor.ignoreTrimWhitespace": false,
	"prettier.jsxSingleQuote": true,
	"github.copilot.enable": {
		"*": true,
		"yaml": false,
		"plaintext": true,
		"markdown": true,
		"javascriptreact": true,
		"javascript": true,
		"html": true
